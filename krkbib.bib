@article{FeZe:HERMMETH1997,
title = "Non-hermitian random matrix theory: Method of hermitian reduction ",
journal = "Nuclear Physics B ",
volume = "504",
number = "3",
pages = "579--608",
year = "1997",
note = "",
issn = "0550-3213",
doi = "http://dx.doi.org/10.1016/S0550-3213(97)00502-6",
url = "http://www.sciencedirect.com/science/article/pii/S0550321397005026",
author = "Joshua Feinberg and A. Zee",
keywords = "Non-hermitian random matrices",
keywords = "Non-gaussian ensembles",
keywords = "Deterministic plus random",
keywords = "The method of hermitization",
keywords = "Non-hermitian hamiltonians and localization ",
abstract = "We consider random non-hermitian matrices in the large-N limit. The power of analytic function theory cannot be brought to bear directly to analyze non-hermitian random matrices, in contrast to hermitian random matrices. To overcome this difficulty, we show that associated to each ensemble of non-hermitian matrices there is an auxiliary ensemble of random hermitian matrices which can be analyzed by the usual methods. We then extract the Green function and the density of eigenvalues of the non-hermitian ensemble from those of the auxiliary ensemble. We apply this “method of hermitization” to several examples, and discuss a number of related issues. "
}

